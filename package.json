{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.11.0",
  "description": "A simple, clean and cross-platform music player, written with Node.js, Electron and React.js.",
  "main": "./dist/main/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/martpie/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martpie/museeks/issues"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js --env.production --colors",
    "lint:ts": "tslint -p ./tsconfig.json -t stylish",
    "lint:css": "stylelint \"src/**/*.css\"",
    "dev": "webpack --config webpack.dev.js --colors --watch",
    "museeks": "./node_modules/.bin/electron .",
    "museeks:debug": "./node_modules/.bin/electron . --enable-logging --devtools",
    "modules:rebuild": "electron-rebuild -f",
    "package:lmw": "build -lmw --ia32 --x64",
    "package:l": "build -l --ia32 --x64",
    "package:w": "build -w --ia32 --x64",
    "package:m": "build -m"
  },
  "dependencies": {
    "bluebird": "3.5.3",
    "chardet": "0.7.0",
    "classnames": "2.2.6",
    "font-awesome": "4.7.0",
    "globby": "9.1.0",
    "history": "4.9.0",
    "iconv-lite": "0.4.24",
    "level-js": "4.0.0",
    "linvodb3": "3.26.0",
    "lodash-es": "4.17.11",
    "m3ujs": "0.2.1",
    "mime-types": "2.1.22",
    "mpris-service": "2.0.0",
    "music-metadata": "3.5.3",
    "postcss-scss": "2.0.0",
    "ps-node": "0.1.6",
    "queue": "6.0.1",
    "react": "16.8.6",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.8.6",
    "react-fontawesome": "1.6.1",
    "react-keybinding-component": "1.0.0",
    "react-onclickout": "2.0.8",
    "react-rangeslider": "2.2.0",
    "react-redux": "6.0.1",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "react-simple-input": "0.4.2",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "semver": "6.0.0",
    "svg-inline-react": "3.1.0",
    "teeny-conf": "4.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.26",
    "@types/chardet": "0.5.0",
    "@types/classnames": "2.2.7",
    "@types/history": "4.7.2",
    "@types/lodash-es": "4.17.3",
    "@types/mime-types": "2.1.0",
    "@types/react": "16.8.8",
    "@types/react-custom-scrollbars": "4.0.5",
    "@types/react-dom": "16.8.3",
    "@types/react-fontawesome": "1.6.4",
    "@types/react-redux": "7.0.5",
    "@types/react-router-dom": "4.3.1",
    "@types/redux-logger": "3.0.7",
    "@types/semver": "5.5.0",
    "clean-terminal-webpack-plugin": "2.0.1",
    "css-loader": "2.1.1",
    "electron": "4.1.1",
    "electron-builder": "20.39.0",
    "electron-rebuild": "1.8.4",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "install": "0.12.2",
    "mini-css-extract-plugin": "0.5.0",
    "node-loader": "0.6.0",
    "normalize.css": "8.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
