{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.23.0",
  "description": "A simple, clean, and cross-platform music player.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/martpie/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martpie/museeks/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "gen:types": "bash scripts/gen.sh",
    "gen:translations": "lingui extract --clean --overwrite",
    "test:typings": "tsc --noEmit --project ./tsconfig.json",
    "test:unit": "vitest run",
    "test:lint": "biome ci .",
    "test:lint:fix": "biome check --write .",
    "package:checksums": "bash scripts/checksum.sh",
    "clippy": "cargo clippy --manifest-path src-tauri/Cargo.toml",
    "release": "bash scripts/release.sh",
    "release:flatpak:build": "flatpak-builder --force-clean --user --disable-cache --repo release/flatpak-repo release/flatpak src-tauri/release/io.museeks.app.yaml",
    "release:flatpak:install": "npm run release:flatpak:build --install",
    "release:flatpak:package": "flatpak build-bundle release/flatpak-repo release/museeks.flatpak io.museeks.app",
    "release:flatpak:lint": "flatpak run --command=flatpak-builder-lint org.flatpak.Builder manifest src-tauri/release/io.museeks.app.yaml && flatpak run --command=flatpak-builder-lint org.flatpak.Builder repo release/flatpak-repo --exceptions --user-exceptions src-tauri/release/flatpak.exceptions.json",
    "safetest": "OPT_URL=${OPT_URL:-http://localhost:1420/} vitest --config vite.safetest.config",
    "safetest:ci": "rm -f artifacts.json && OPT_URL=${DEPLOYED_URL} OPT_CI=1 OPT_DOCKER=1 OPT_ARTIFACTS=artifacts.json npm run safetest -- --run --bail=5",
    "safetest:regenerate-screenshots": "OPT_DOCKER=1 npm run safetest -- --run --update"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@lingui/core": "5.4.0",
    "@lingui/react": "5.4.0",
    "@tanstack/react-query": "5.84.1",
    "@tanstack/react-router": "1.130.12",
    "@tanstack/react-virtual": "3.13.12",
    "@tauri-apps/api": "2.7.0",
    "@tauri-apps/plugin-dialog": "2.3.2",
    "@tauri-apps/plugin-fs": "2.4.1",
    "@tauri-apps/plugin-log": "2.6.0",
    "@tauri-apps/plugin-notification": "2.3.0",
    "@tauri-apps/plugin-opener": "2.4.0",
    "@tauri-apps/plugin-os": "2.3.0",
    "browserslist": "4.25.1",
    "classnames": "2.5.1",
    "lodash-es": "4.17.21",
    "nanoid": "5.1.5",
    "normalize.css": "8.0.1",
    "radix-ui": "1.4.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-keybinding-component": "2.2.0",
    "semver": "7.7.2",
    "zustand": "5.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@lingui/babel-plugin-lingui-macro": "5.4.0",
    "@lingui/cli": "5.4.0",
    "@lingui/vite-plugin": "5.4.0",
    "@tanstack/router-plugin": "1.130.12",
    "@tauri-apps/cli": "2.7.1",
    "@types/lodash-es": "4.17.12",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@types/semver": "7.7.0",
    "@vitejs/plugin-react": "4.7.0",
    "jest": "30.0.5",
    "lightningcss": "1.30.1",
    "safetest": "0.1.13",
    "typescript": "5.9.2",
    "typescript-plugin-css-modules": "5.2.0",
    "vite": "7.0.6",
    "vite-plugin-svgr": "4.3.0",
    "vitest": "3.2.4"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@parcel/watcher",
    "esbuild"
  ]
}
