{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.13.1",
  "description": "A simple, clean and cross-platform music player.",
  "main": "dist/main/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/martpie/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martpie/museeks/issues"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "vite build",
    "dev": "vite",
    "museeks": "electron .",
    "museeks:debug": "electron . --enable-logging --devtools",
    "test:typings": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:lint": "eslint . --ext .tsx,.ts,.js",
    "test:lint:fix": "eslint . --ext .tsx,.ts,.js --fix",
    "test:formatting": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "test:formatting:fix": "prettier --write \"./**/*.{ts,tsx,js}\"",
    "test:css": "stylelint \"src/**/*.css\"",
    "package:lmw": "electron-builder -lmw && yarn run package:checksums",
    "package:lw": "electron-builder -lw",
    "package:l": "electron-builder -l",
    "package:w": "electron-builder -w",
    "package:m": "electron-builder -m",
    "package:checksums": "bash scripts/checksum.sh"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@reduxjs/toolkit": "^1.9.1",
    "bluebird": "^3.7.2",
    "chardet": "^1.5.0",
    "classnames": "^2.3.2",
    "electron-devtools-installer": "^3.2.0",
    "font-awesome": "^4.7.0",
    "globby": "^11.1.0",
    "history": "^5.3.0",
    "iconv-lite": "^0.6.3",
    "level-js": "^4.0.2",
    "linvodb3": "^3.26.0",
    "lodash-es": "^4.17.21",
    "m3ujs": "^0.2.1",
    "mime-types": "^2.1.35",
    "music-metadata": "^7.13.0",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "postcss-scss": "^4.0.6",
    "postcss-url": "^10.1.3",
    "ps-node": "^0.1.6",
    "queue": "^6.0.2",
    "rc-scrollbars": "^1.1.5",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-fontawesome": "^1.7.1",
    "react-keybinding-component": "^1.0.0",
    "react-onclickout": "^2.0.8",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^8.0.5",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "semver": "^7.3.8",
    "svg-inline-react": "^3.2.1",
    "teeny-conf": "^5.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/lodash-es": "^4.17.6",
    "@types/mime-types": "^2.1.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.0.0",
    "electron": "18.3.6",
    "electron-builder": "^23.6.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "install": "^0.13.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.20",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.8.1",
    "prettier-eslint": "^15.0.1",
    "redux-logger": "^3.0.6",
    "stylelint": "^14.16.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.9.4",
    "typescript-plugin-css-modules": "^4.1.1",
    "vite": "^4.0.2",
    "vite-plugin-electron": "^0.9.2",
    "vite-plugin-electron-renderer": "^0.9.0",
    "vitest": "^0.25.8"
  }
}
