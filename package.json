{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.23.1",
  "description": "A simple, clean, and cross-platform music player.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/martpie/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martpie/museeks/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "gen:types": "bash scripts/gen.sh",
    "gen:translations": "lingui extract --clean --overwrite",
    "test:typings": "tsc --noEmit --project ./tsconfig.json",
    "test:unit": "vitest run --project unit",
    "test:ui": "vitest run --project e2e",
    "test:ui:watch": "vitest watch --project e2e",
    "test:lint": "biome ci .",
    "test:lint:fix": "biome check --write .",
    "package:checksums": "bash scripts/checksum.sh",
    "clippy": "cargo clippy --manifest-path src-tauri/Cargo.toml",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@lingui/core": "5.5.2",
    "@lingui/react": "5.5.2",
    "@tanstack/react-query": "5.90.5",
    "@tanstack/react-router": "1.133.36",
    "@tanstack/react-virtual": "3.13.12",
    "@tauri-apps/api": "2.9.0",
    "@tauri-apps/plugin-clipboard-manager": "2.3.2",
    "@tauri-apps/plugin-dialog": "2.4.2",
    "@tauri-apps/plugin-fs": "2.4.4",
    "@tauri-apps/plugin-log": "2.7.1",
    "@tauri-apps/plugin-notification": "2.3.3",
    "@tauri-apps/plugin-opener": "2.5.2",
    "@tauri-apps/plugin-os": "2.3.2",
    "@tauri-apps/plugin-process": "2.3.1",
    "browserslist": "4.27.0",
    "classnames": "2.5.1",
    "eventemitter3": "5.0.1",
    "lodash-es": "4.17.21",
    "nanoid": "5.1.6",
    "normalize.css": "8.0.1",
    "radix-ui": "1.4.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-keybinding-component": "2.2.0",
    "semver": "7.7.3",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@lingui/babel-plugin-lingui-macro": "5.5.2",
    "@lingui/cli": "5.5.2",
    "@lingui/vite-plugin": "5.5.2",
    "@tanstack/router-plugin": "1.133.36",
    "@tauri-apps/cli": "2.9.2",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/semver": "7.7.1",
    "@vitejs/plugin-react": "5.1.0",
    "@vitest/browser": "4.0.5",
    "@vitest/browser-playwright": "4.0.5",
    "babel-plugin-react-compiler": "1.0.0",
    "lightningcss": "1.30.2",
    "playwright": "1.56.1",
    "simple-git-hooks": "2.13.1",
    "typescript": "5.9.3",
    "typescript-plugin-css-modules": "5.2.0",
    "vite": "7.1.12",
    "vite-plugin-node-polyfills": "0.24.0",
    "vite-plugin-svgr": "4.5.0",
    "vitest": "4.0.5",
    "vitest-browser-react": "2.0.2"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@parcel/watcher",
    "esbuild"
  ],
  "simple-git-hooks": {
    "pre-commit": "npm run gen:translations && git add src/translations"
  }
}
