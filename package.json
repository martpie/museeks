{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.13.1",
  "description": "A simple, clean and cross-platform music player.",
  "main": "dist/main/entrypoint.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/martpie/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martpie/museeks/issues"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-vite build",
    "dev": "electron-vite dev --watch",
    "start": "electron-vite preview",
    "museeks": "electron .",
    "museeks:debug": "electron . --enable-logging --devtools",
    "test:typings": "yarn run test:typings:root && yarn run test:typings:main && yarn run test:typings:preload && yarn run test:typings:renderer",
    "test:typings:root": "tsc --noEmit --project ./tsconfig.json",
    "test:typings:main": "tsc --noEmit --project src/main/tsconfig.json",
    "test:typings:preload": "tsc --noEmit --project src/preload/tsconfig.json",
    "test:typings:renderer": "tsc --noEmit --project src/renderer/tsconfig.json",
    "test:unit": "vitest run",
    "test:lint": "eslint . --ext .tsx,.ts,.js",
    "test:lint:fix": "eslint . --ext .tsx,.ts,.js --fix",
    "test:formatting": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "test:formatting:fix": "prettier --write \"./**/*.{ts,tsx,js}\"",
    "test:css": "stylelint \"src/**/*.css\"",
    "package:lmw": "electron-builder -lmw && yarn run package:checksums",
    "package:lw": "electron-builder -lw",
    "package:l": "electron-builder -l",
    "package:w": "electron-builder -w",
    "package:m": "electron-builder -m",
    "package:checksums": "bash scripts/checksum.sh"
  },
  "dependencies": {
    "@bscop/use-click-out": "^1.3.1",
    "@electron/remote": "^2.0.9",
    "bluebird": "^3.7.2",
    "chardet": "^1.5.1",
    "classnames": "^2.3.2",
    "electron-devtools-installer": "^3.2.0",
    "font-awesome": "^4.7.0",
    "globby": "^13.1.4",
    "iconv-lite": "^0.6.3",
    "level-js": "^4.0.2",
    "linvodb3": "^3.26.0",
    "lodash": "^4.17.21",
    "m3ujs": "^0.2.1",
    "music-metadata": "^7.13.4",
    "nanoid": "^4.0.2",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "postcss-scss": "^4.0.6",
    "postcss-url": "^10.1.3",
    "queue": "^7.0.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-fontawesome": "^1.7.1",
    "react-keybinding-component": "^1.0.0",
    "react-rangeslider": "^2.2.0",
    "react-router-dom": "6.11.2",
    "react-virtuoso": "^4.3.7",
    "semver": "^7.5.1",
    "svg-inline-react": "^3.2.1",
    "teeny-conf": "^6.2.3",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/lodash": "^4.14.195",
    "@types/mime-types": "^2.1.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "electron": "24.4.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.23",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "stylelint": "^15.6.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  }
}
