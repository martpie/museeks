    "stylelint-config-css-modules": "1.3.0",
    "stylelint-config-standard": "18.2.0",
    "svg-inline-loader": "0.8.0",
    "terser-webpack-plugin": "1.2.3",
    "ts-loader": "5.3.3",
    "tslint": "5.14.0",
    "tslint-config-semistandard": "7.0.0",
    "typescript": "3.3.4000",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-merge": "4.2.1",
    "webpackbar": "3.1.5"
  }
}
